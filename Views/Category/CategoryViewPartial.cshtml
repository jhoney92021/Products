@using Products.Models
@model List<Category>

<h3>Exsisting Products</h3>
<ul style="list-style: none;">
    @foreach(Category category in @Model)
    {
        <li>
            <a asp-action="ViewCategory" asp-controller="Category" asp-route-id="@category.CategoryId">@category.Name</a>
        </li>
    }
</ul>